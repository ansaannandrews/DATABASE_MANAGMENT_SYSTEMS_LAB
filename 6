SELECT CEIL(8.29) FROM DUAL;
SELECT CEIL(-3.9) FROM DUAL;
SELECT CEIL(17/3) FROM DUAL;
SELECT FLOOR(9.76) FROM DUAL;
SELECT FLOOR(-5.3) FROM DUAL;
SELECT FLOOR(15/4) FROM DUAL;
SELECT SQRT(625) FROM DUAL;
SELECT SQRT(2) FROM DUAL;
SELECT abs(8.29) FROM DUAL;
SELECT abs(-3.14) FROM DUAL;
SELECT abs(-42) FROM DUAL;
SELECT SYSDATE AS NOW FROM DUAL;
SELECT 
  TO_CHAR(SYSDATE, 'YYYY-MM-DD') AS date_format, 
  TO_CHAR(SYSDATE, 'HH:MI:SS AM') AS time_format 
FROM DUAL;
SELECT SYSTIMESTAMP FROM DUAL;
SELECT LEAST('Apple','Orange','Grapes') FROM DUAL;
SELECT GREATEST('Apple','Orange','Grapes') FROM DUAL;
SELECT TRIM(' Trim me') FROM DUAL;
SELECT TRIM('I love India                ') FROM DUAL;
SELECT RPAD('Ansa', 15, 'X') FROM DUAL;
SELECT RPAD(123, 6, '0') FROM DUAL;
SELECT REVERSE('uoyevol I') FROM DUAL;
SELECT CASE WHEN 'racecar' = REVERSE (racecar THEN 'Palindrome' else 'Not_Palindrome')
END AS RESULT
FROM DUAL;
SELECT CASE 
    WHEN 'racecar' = REVERSE('racecar') THEN 'Palindrome' 
    ELSE 'Not_Palindrome' 
END AS RESULT 
FROM DUAL;
SELECT LENGTH('Oracle Database')
FROM DUAL;
SELECT 'Oracle' || ' ' || 'SQL' || ' ' || 'is' || ' ' || 'powerful' || ' ' || 'and' || ' ' ||  'flexible'
FROM DUAL;
SELECT CONCAT('Ansa','Andrews')
FROM DUAL;
SELECT SUBSTR('India is my country',INSTR('India is my country','is'),2)
FROM DUAL;
SELECT SUBSTR('AnsaAnn', 3, 3)
FROM DUAL;
SELECT SUBSTR('Substring', 1, 3)
FROM DUAL;

