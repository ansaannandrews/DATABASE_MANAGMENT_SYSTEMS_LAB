CREATE TABLE BANK
(   
    bankid INT PRIMARY KEY,
    bankname VARCHAR(20),
    headoffice VARCHAR(20),
    branch VARCHAR(20),
    branchcode VARCHAR(20)
);

INSERT INTO BANK VALUES(1,'State Bank of India','Mumbai','Delhi Main Branch','SBI0001');
